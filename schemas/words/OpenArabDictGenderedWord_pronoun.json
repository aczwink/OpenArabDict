{    
    "$schema": "https://json-schema.org/draft-07/schema#",
    "title": "OpenArabDict pronoun definition",
    "description": "JSON Schema for OpenArabDict pronoun definitions",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "derived": {
            "type": "array",
            "items": {
                "$ref": "./OpenArabDictWord_derived.json"
            }
        },
        "gender": {
            "type": "string",
            "enum": [
                "female",
                "male"
            ]
        },
        "text": {
            "type": "string"
        },
        "type": {
            "const": "pronoun"
        }
    },
    "required": ["gender", "text", "type"]
}